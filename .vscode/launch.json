{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}/web-client"
        },
        {
            "name": "Launch Tst DP",
            "program": "${workspaceFolder}/dist/test/test-dataProvider/index.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "env": {
                "SEQ_LOGGER": "localhost",
                "SEQ_API_KEY": "lx9nIkLTGXe6lHzAUJqH"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/test/nodejs-server/NodeServerTest.js",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "console": "integratedTerminal",
            "env": {
                "SEQ_LOGGER": "localhost"
            }
        },
        {
            "name": "Test Client",
            "program": "${workspaceFolder}/dist/test/nodejs-client/NodeClientTest.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test Server 2",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/test/nodejs-server/NodeServerTest.js",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "args": ["3001"],
            "console": "integratedTerminal"
        }
    ]
}